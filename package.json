{
  "name": "@tomotomy/redash-loader",
  "version": "0.0.1",
  "description": "",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "ts-node --files src/index.ts",
    "cli": "node -r ts-node/register src/index.ts",
    "dev:watch": "ts-node-dev --files --respawn src/index.ts",
    "clean": "rimraf dist/*",
    "tsc": "tsc",
    "build": "npm-run-all clean tsc",
    "start": "node .",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src/**/*.ts",
    "eslint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write 'src/**/*.{js,ts,json}'",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tomotomy/redash-loader.git"
  },
  "publishConfig": {
    "@tomotomy:registry": "https://npm.pkg.github.com"
  },
  "bin": {
    "redash-loader": "./dist/index.js"
  },
  "keywords": [],
  "author": "Tomoya Nakano",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "8.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.6.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "axios": "^0.26.1",
    "dotenv": "^16.0.0",
    "make-dir": "^3.1.0"
  }
}
